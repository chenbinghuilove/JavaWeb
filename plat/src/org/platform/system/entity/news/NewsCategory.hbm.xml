<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 资讯栏目数据库映射 -->
<hibernate-mapping>
	<class name="org.platform.system.entity.news.NewsCategory" table="news_category">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>
		<property name="keywords" type="java.lang.String">
			<column name="keywords" />
		</property>
		<property name="description" type="java.lang.String">
			<column name="description" />
		</property>
		<property name="visible" type="java.lang.Boolean">
			<column name="visible" />
		</property>
		<property name="priority" type="java.lang.Integer">
			<column name="priority" />
		</property>
		<property name="editDate" type="java.util.Date">
			<column name="edit_date" />
		</property>
	</class>
</hibernate-mapping>
